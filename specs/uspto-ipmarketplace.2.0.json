        },
        "patentAvailabilityIndicator": {
          "type": "string",
          "example": "Is the invention patented or not"
        },
        "patentNumber": {
          "type": "string",
          "example": "Patent Number"
        },
        "pgPublicationAvailabilityIndicator": {
          "type": "string",
          "example": "Is the invention published or not"
        },
        "publicationDate": {
          "type": "string",
          "example": "Publication date"
        },
        "publicationNumber": {
          "type": "string",
          "example": "Publication Number"
        },
        "recordIdentifier": {
          "type": "string",
