            }
          }
        },
        "parameterData": {
          "type": "object",
          "example": "Provide parameter name and its value for search",
          "additionalProperties": {
            "type": "string"
          }
        },
        "recordStartNumber": {
          "type": "integer",
          "format": "int32",
          "example": "Specify from which record the search should start"
        },
        "recordTotalQuantity": {
          "type": "integer",
          "format": "int32",
          "example": "Specify how many number of results to be brought from search"
        },
        "searchText": {
          "type": "string",
          "example": "Search text to search all the results"
        },
        "sortDataBag": {
          "type": "array",
          "example": "Provide sort columns and orders so that results are sorted",
          "items": {
            "$ref": "#/definitions/Map«string,string»"
          }
        }
      }
    },
    "SearchOutput": {
      "type": "object",
      "properties": {
        "aggregationData": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/AggregationInfo"
            }
          }
        },
        "recordTotalQuantity": {
          "type": "integer",
          "format": "int64"
        },
        "responseMessage": {
          "type": "string"
        },
        "results": {
          "type": "object"
        }
      }
