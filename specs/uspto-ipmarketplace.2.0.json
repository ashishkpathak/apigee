        "assigneeNameText": {
          "type": "array",
          "example": "Assignee lsit of the patent",
          "items": {
            "type": "string"
          }
        },
        "firstInventorName": {
          "type": "string",
          "example": "Name of the first inventor of the invention"
        },
        "grantDate": {
          "type": "string",
          "example": "Patent Issue Date"
        },
        "inventionAbstractText": {
          "type": "string",
          "example": "Abstract of the invention"
        },
        "inventionTitle": {
          "type": "string",
          "example": "Title of the invention"
        },
        "inventorNameText": {
          "type": "array",
          "example": "Other inventors list",
