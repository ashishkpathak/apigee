    "FacetMapInput": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        }
      }
    },
    "Map«string,string»": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "SearchInput": {
      "type": "object",
      "properties": {
        "dateRangeData": {
