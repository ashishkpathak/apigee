      "post": {
        "tags": ["USPTO IP MARKET PLACE Patent Search"],
        "summary": "Provides USPTO IP MARKET PLACE patent information",
        "description": "USPTO IP MARKET PLACE data",
        "operationId": "searchCovidDataWithFiltersUsingPOST",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [{
          "in": "body",
          "name": "searchInput",
          "description": "Searches Ip Market Place data.",
          "required": true,
          "schema": {
            "$ref": "#/definitions/SearchInput"
          }
        }],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CovidDataSearchRecord"
              }
            }
          },
          "401": {
            "description": "Requestor must authenticate in order to access resource"
          },
          "404": {
            "description": "Resouce not found or query did not return any data"
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
    "/search/query": {
      "get": {
        "tags": ["USPTO IP MARKET PLACE Patent Search"],
        "summary": "Provides USPTO IP MARKET PLACE patent information",
        "description": "USPTO IP MARKET PLACE data",
        "operationId": "searchCovidDataWithQueryUsingGET",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [{
          "name": "searchText",
          "in": "query",
          "description": "Search Text",
          "required": false,
          "type": "string"
        }],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CovidDataSearchRecord"
              }
            }
          },
          "401": {
            "description": "Requestor must authenticate in order to access resource"
          },
          "404": {
            "description": "Resouce not found or query did not return any data"
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    }
  },
