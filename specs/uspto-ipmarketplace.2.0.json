{
  "swagger": "2.0",
  "info": {
    "description": "The first version of the USPTO IP MARKET PLACE Patent information System REST API.",
    "version": "1.0",
    "title": "USPTO IP MARKET PLACE Patent Information System REST Services",
    "contact": {
      "name": "ODD Team"
    }
  },
  "host": "developer.uspto.gov",
  "basePath": "/ipmarketplace-api",
  "tags": [{
    "name": "USPTO IP MARKET PLACE Patent Search",
    "description": "USPTO IP MARKET PLACE Patent Information"
  }],
  "paths": {
